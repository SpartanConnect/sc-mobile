{
  "expo": {
    "name": "Spartan Connect",
    "description": "Spartan Connect is a mobile app for LCHS students to read announcements.",
    "slug": "sc-mobile",
    "privacy": "public",
    "sdkVersion": "19.0.0",
    "version": "0.14",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spartanconnect.SpartanConnect"
    },
    "android": {
      "package": "com.spartanconnect.SpartanConnect",
      "permissions": ["READ_INTERNAL_STORAGE", "READ_PHONE_STATE"]
    }
  }
}
